{
  "name": "maryamnbyn/vandar-laravel-ipg",
  "description": "Laravel Library for Vandar Gateway",
  "minimum-stability": "stable",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Maryam Nabiyan",
      "email": "maryam.nbyn@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Mohammad H. Mazalahi",
      "email": "mohammadhoseinmazalahi@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "docs": "https://docs.vandar.io"
  },
  "scripts": {
    "tests": "vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "Vandar\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vandar\\Laravel\\VandarServiceProvider"
      ],
      "aliases": {
        "Vandar": "Vandar\\Laravel\\Facade\\Vandar"
      }
    }
  },
  "require-dev": {
    "orchestra/testbench": "^4.0 | ^5.0 | ^6.0 | ^7.0"
  },
  "require": {
    "ext-json": "*",
    "ext-curl": "*"
  }
}
